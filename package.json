{
  "name": "more-recipes",
  "version": "1.0.0",
  "description": "More-Recipes provides a platform for users to share the awesome and exciting recipe ideas they have invented or learnt.\r Suppose a user comes up with a recipe, he/she can post it on More-Recipes and get feedback in form of reviews and votes from other users who explore that recipe. Users can also keep a list of their favorite recipes on the application.",
  "main": "app.js",
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development nodemon --exec babel-node server/bin/www",
    "test": "cross-env NODE_ENV=test npm run reset-migrate && cross-env NODE_ENV=test mocha server/test/*.js --exit --require babel-register --reporter spec",
    "build": "babel ./server/ -d dist/server",
    "start": "babel-node server/bin/www",
    "heroku-postbuild": "npm run build",
    "migrate": "sequelize db:migrate",
    "reset-migrate": "sequelize db:migrate:undo:all&&sequelize db:migrate",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc npm test&& nyc report --reporter=text-lcov | coveralls",
    "build:client": "webpack",
    "dev": "webpack-dev-server",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/missating/More-Recipes.git"
  },
  "author": "Vanessa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/missating/More-Recipes/issues"
  },
  "homepage": "https://github.com/missating/More-Recipes#readme",
  "dependencies": {
    "axios": "^0.17.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.0.0-beta",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "cors": "^2.8.4",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.1",
    "dotenv": "^4.0.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "express": "^4.16.2",
    "faker": "^4.1.0",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "lodash.isempty": "^4.4.0",
    "mocha": "^4.0.1",
    "morgan": "^1.9.0",
    "nodemon": "^1.12.1",
    "nyc": "^11.2.1",
    "path": "^0.12.7",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.6.0",
    "react": "^16.1.0",
    "react-dom": "^16.1.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
